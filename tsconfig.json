{
  "compilerOptions": {
      "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
  
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "types": ["vite/client"],
     "skipLibCheck": true,
  },
  "include": ["src"]
}